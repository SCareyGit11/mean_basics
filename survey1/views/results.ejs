<html>
	<head>
	<meta charset="utf-8">
	<title>Survey 1</title>
	</head>
	<body>
		    <h2>Here are all the users:</h2>
    	<% for (var x in users) { %>
        <h3>Name: <%= users[x].name %></h3>
        <h3>Location: <%= users[x].location %></h3>
        <h3>Language: <%= users[x].language %></h3>
        <h3>Comment: <%= users[x].comment %></h3>
        <h4>Email: <%= users[x].email %></h4>
        <hr>
    <% } %>
    <button><a href="/">Back,Back,Back</a></button>
	</body>
</html>