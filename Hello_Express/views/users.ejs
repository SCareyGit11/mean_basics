<html>
<body>
    <h2>Here are all the users:</h2>
    <% for (var x in users) { %>
        <h3>Name: <%= users[x].name %></h3>
        <h4>Email: <%= users[x].email %></h4>
        <form action="/users/1" method="get">
        	<input type="submit" name="submit">
        </form>
        <hr>
    <% } %>
</body>
</html>